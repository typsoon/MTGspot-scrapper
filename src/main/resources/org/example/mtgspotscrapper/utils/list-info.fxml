<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<!--suppress JavaFxDefaultTag -->
<VBox fx:id="vBox" alignment="TOP_CENTER" spacing="5.0" style="-fx-background-color: rgba(0,0,0,0);" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imageView" fitHeight="340" fitWidth="244" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/wastes.jpeg" />
         </image></ImageView>
      <HBox alignment="CENTER" spacing="15.0">
         <children>
            <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" maxWidth="200.0" prefHeight="19.0" prefWidth="200.0" text="Label">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
