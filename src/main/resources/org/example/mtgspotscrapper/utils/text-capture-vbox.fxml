<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<!--suppress JavaFxDefaultTag -->
<VBox alignment="TOP_CENTER" prefHeight="501.0" prefWidth="603.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="listNameTextField" alignment="CENTER" maxWidth="-Infinity" prefHeight="24.0" prefWidth="268.0" promptText="List name" />
      <TextArea fx:id="textArea" prefHeight="432.0" prefWidth="563.0" promptText="Copy your deck in here and click enter" />
      <Label fx:id="captureTextLabel" text="Import">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
